  <div id="page-title">
    <h3>{{model.header}}</h3>
  </div>
  <div id="page-content">
    <form>
      <div class="form-group">
        <label >Title</label>
        <input class="form-control" name="title" value={{model.header}}/>
      </div>
      <div class="form-group">
        <label >Body</label>
        <input class="form-control" name="title" value={{model.body}} />
      </div>

    </form>

    <hr/>
    <pre>{{object-to-pre model}}</pre>

  </div>



